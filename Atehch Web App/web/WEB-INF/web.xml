<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CreateAccountServlet</servlet-name>
        <servlet-class>atech.servlets.ac.za.CreateAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashBaordServlet</servlet-name>
        <servlet-class>atech.servlets.ac.za.DashBaordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogInServlet</servlet-name>
        <servlet-class>atech.servlets.ac.za.LogInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddItemServlet</servlet-name>
        <servlet-class>atech.servlets.ac.za.AddItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetProductServelt</servlet-name>
        <servlet-class>atech.servlets.ac.za.GetProductServelt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddGpuServlet</servlet-name>
        <servlet-class>atech.servlets.ac.za.AddGpuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WishListServlet</servlet-name>
        <servlet-class>atech.servlets.ac.za.WishListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateAccountServlet</servlet-name>
        <url-pattern>/CreateAccountServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashBaordServlet</servlet-name>
        <url-pattern>/DashBaordServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogInServlet</servlet-name>
        <url-pattern>/LogInServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddItemServlet</servlet-name>
        <url-pattern>/AddItemServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetProductServelt</servlet-name>
        <url-pattern>/GetProductServelt.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddGpuServlet</servlet-name>
        <url-pattern>/AddGpuServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WishListServlet</servlet-name>
        <url-pattern>/WishListServlet.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
